<div class="test-container">
	<h2>Sample Form</h2>
	<form action="/test-verifikasi" method="post">
		<div class="form-group">
			<label for="name">Name:</label>
			<input
				type="text"
				id="name"
				name="privateKey"
				value="<%= privateKey %>"
				placeholder=""
				required
				hidden />
		</div>
		<div class="form-group">
			<label for="message">Message:</label>
			<textarea
				id="message"
				name="message"
				rows="4"
				required
				value="<%= message %>"></textarea>
		</div>
		<button type="submit">Verify</button>
	</form>
</div>
