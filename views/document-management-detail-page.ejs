<div class="file-target" get-data="<%= fileTarget %>"></div>
<div class="data-user" get-data="<%= dataUser %>"></div>
<div class="wrapper">
	<div class="box-info">
		<div class="box-status">
			<div class="title-status">Signing Status</div>
			<div class="signing-status"></div>
		</div>
		<div class="box-status">
			<div class="title-status">Verification Status</div>
			<div class="verify-status"></div>
		</div>
		<div class="box-doc-info">
			<div class="box-name">Document Owner</div>
			<div class="box-content">
				<div class="field">
					<div class="label">Name</div>
					<div class="content" id="owner-name"></div>
				</div>
				<div class="field">
					<div class="label">Email</div>
					<div class="content" id="owner-email"></div>
				</div>
			</div>
		</div>
		<div class="box-doc-info">
			<div class="box-name">Document Sender</div>
			<div class="box-content">
				<div class="field">
					<div class="label">Name</div>
					<div class="content" id="sender-name"></div>
				</div>
				<div class="field">
					<div class="label">Email</div>
					<div class="content" id="sender-email"></div>
				</div>
			</div>
		</div>
	</div>

	<div class="doc-container">
		<iframe src="" id="doc-display"></iframe>
	</div>
	<div class="send-wrapper">
		<div class="box-send">
			<div class="box-title">Send Document</div>
			<div class="box-reciver">
				<div class="btn-add"><i class="bx bx-plus"></i></div>
			</div>
		</div>
		<div class="btn-send">Send</div>
	</div>
	<div class="box-action">
		<div class="btn-sign">Sign</div>
		<div class="btn-verify">Verify</div>
	</div>
</div>
<%- include('layouts/search-user-modal') %>
